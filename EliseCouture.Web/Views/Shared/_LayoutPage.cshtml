<!DOCTYPE html>
<html>
    <head>
        <title>@ViewBag.Title - Amory Fitness</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="/css/bootstrap.css" rel="stylesheet" media="screen">
        <link href="/css/style.css" rel="stylesheet" media="screen">
        <link href="http://fonts.googleapis.com/css?family=Josefin+Sans|Pacifico" rel="stylesheet" type="text/css">
    </head>
    <body>
        <div id="header" style="">
            <div class="content">
                <div id="logo">
                    <a href="/">AMORY<br />FITNESS</a>
                </div>
                    
                <div id="sections">
                    <ul class="unstyled inline">
                        <li>@Html.ActionLink("services", "index", "services")</li>
                        <li>@Html.ActionLink("blog", "index", "blog")</li>
                        <li>@Html.ActionLink("community", "index", "community")</li>
                        <li>@Html.ActionLink("contact", "index", "contact")</li>
                    </ul>
                </div>
            </div>
        </div>

        @RenderBody()
    
        <script src="http://code.jquery.com/jquery.js"></script>
        <script src="/js/bootstrap.min.js"></script>
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-40141521-1', 'elisecouture.com');
            ga('send', 'pageview');
        </script>
    </body>
</html>